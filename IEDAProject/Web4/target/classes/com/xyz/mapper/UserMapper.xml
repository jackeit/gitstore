<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xyz.mapper.UserMapper">
    <select id="selectusers" resultType="com.xyz.mybatistest.User">
        select * from Test1 ;
    </select>
    <insert id="insertuser" keyProperty="id" useGeneratedKeys="true" parameterType="com.xyz.mybatistest.User" >
        insert into test1 (age,class_id,name,hobbies) values(#{age},#{class_id},#{name},#{hobbies});
    </insert>
</mapper>